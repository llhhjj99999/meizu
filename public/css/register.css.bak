.wrapper{
    height: 100%;
    background-color: #f9f9f9;
}
.layout{
    position: relative;
    top:20px;
    width: 854px;
    min-height: 540px;
    margin: 40px auto;
    text-align: center;
    background-color:#fff;
}
.regbox {
    width: 332px;
    padding: 20px 0;
    margin: 0 auto;
    position: relative;
    /*background-color: green;*/
}
.bigtitle{
    margin:10px ;
    font-size: 30px;
    font-weight: normal;
}
.seltitle{
    text-align: left;
    font-size: 14px;
    height: 40px;
    line-height: 40px;
}
.phone{
    position: relative;
}
.phone .zone-number{
    position: absolute;
    left: 0;
    display: inline-block;
    width: 80px;
    padding-right: 40px;
    height: 40px;
    line-height: 40px;
    text-align: center;
    border: 1px solid #b8b8b8;
    cursor: pointer;
}
.phone img{
    position: absolute;
    left: 50px;
    top: 18px;
    cursor: pointer;
}
.phone-number{
    position: absolute;
    left: 79px;
    border: 1px solid #b8b8b8;
    width: 253px;
    height: 40px;
    padding-left: 10px;
}
.country-list{
    padding-left: 10px;
    width: 332px;
    border: 1px solid #b8b8b8;
    cursor: pointer;
    height: 40px;
}
.tip{
    font-style:italic;
    text-align: left;
}
.zone-list{
    position: absolute;
    top:186px;
    /*height: 170px;*/
    height:0;
    overflow: hidden;
    width: 332px;
    text-align: right;
    border: 1px solid #b8b8b8;
    border-top: none;
    z-index:100;
    background:#fff;
    padding:0;
}
.zone-list li{
    width: 330px;
    height: 40px;
    padding: 0;
    cursor: pointer;
    position: relative;
}
.zone-list li span:first-child{
    position: absolute;
    top:10px;
    left:50px;
}
.zone-list li span:nth-child(2){
    position: absolute;
    top:10px;
    right:20px;
}
.btn-login{
    height: 40px;
    position: relative;
    top: 70px;
    color: #fff;
    width: 332px;
    background-color: #ff6700;
}
.confirm{
    position: relative;
    top:130px;
}
.confirm p{
    display: inline;
}
.confirm span{
    color: #000;
    font-weight: bold;
}
.account{
    display: block;
    margin: 0 auto;
}
.set-upwd{
    position: relative;
    width: 332px;
    margin: 0 auto;
    text-align: left;
    display: none;
}
.set-upwd .text{
    margin:30px 0;
}
.set-upwd .reg-name{
    color: #ff6700;
}
.set-upwd .msg{
    margin: 20px 0;
}

.set-upwd .upwd{
    border: 1px solid #e0e0e0;
    width: 230px;
    height: 40px;
    padding-left: 10px;
}
.set-upwd .new-msg{
    height: 40px;
    width: 80px;
    margin-left: 18px;
    background-color: #fff;
    border: 1px solid #b8b8b8;
}
.set-upwd .new-msg:hover{
    background: #ececec !important;
    color: #888;
}
.set-upwd .back{
    position: relative;
    height: 40px;
    top: 90px;
    color: #7c7c7c;
    width: 332px;
    background-color: #fff;
    border:1px solid #b8b8b8;;
}
.set-upwd .back:hover{
    color: #333;
}
.n_footer{
    position: relative;
    margin-top: 20px;
    height: 80px;
    margin:0 auto;
    text-align: center;
}
.n_footer>.nf-link-area{
    text-align: center;
}

.n_footer>.nf-link-area>.lang-select-list{
    display:flex;
    justify-content:center;
    style-list:none;
}
.n_footer>.nf-link-area>.lang-select-list>li>a{
    display: inline-block;
    padding:10px;
    color: #757575;
}
.n_footer>.nf-link-area>.lang-select-list>li>a.current:hover{
    color: #333;
}
.n_footer>.nf-intro>.beian-record-link:hover{
    color: #757575;
}
.tip1{
    position: absolute;
    top: 50px;
    height: 20px;
    left: 80px;
    color:#FF6700;
}
.userpwd1{
    height: 20px;
    width: 100px;
    color:red;
}